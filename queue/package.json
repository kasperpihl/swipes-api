{
  "name": "SwipesAPI",
  "version": "0.0.1",
  "description": "Swipes REST API",
  "author": "Tihomir Valkanov <tihomir@swipesapp.com>",
  "dependencies": {
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "bluebird": "^3.5.0",
    "body-parser": "1.x",
    "config": "^1.26.2",
    "cors": "^2.8.4",
    "express": "4.15.4",
    "express-session": "^1.15.5",
    "mailchimp-api-v3": "^1.7.0",
    "mandrill-api": "^1.0.45",
    "moment": "^2.18.1",
    "request": "~2.81.0",
    "request-promise": "4.2.1",
    "rethinkdb": "^2.3.3",
    "underscore": "1.8.x",
    "valjs": "0.0.28"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "nodemon": "^1.12.0",
    "npm-run-all": "^4.1.1",
    "pm2": "2.6.1",
    "rimraf": "^2.6.1",
    "webpack": "^1.13.0"
  },
  "scripts": {
    "build": "babel src -d dist",
    "watch": "babel src -d dist --watch --skip-initial-build",
    "dev": "nodemon ./dist/index.js",
    "prestart": "rimraf ./dist",
    "start": "npm run build && npm-run-all --parallel dev watch",
    "staging": "node ./dist/index.js",
    "live": "node ./dist/index.js",
    "deploy": "npm run build && eb deploy",
    "deploy-live": "npm run build && eb deploy swipesqueue-live"
  }
}
