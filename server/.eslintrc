{
  "extends": "airbnb-base",
  "rules": {
    "no-console": 0,
    "arrow-body-style": 0,
    "camelcase": 0,
    "no-shadow": 0,
    "import/prefer-default-export": 0,
    "no-param-reassign": ["error", { "props": false }],
    "no-underscore-dangle": ["error", { "allow": ["_conn"] }],
    "no-unused-vars": ["error", { "args": "none" }],
  },
}
