{
  "settings": {
    "import/resolver": {
      "babel-module": {
        "root": ["./src"]
      }
    }
  },
  "extends": "airbnb-base",
  "rules": {
    "no-console": 0,
    "arrow-body-style": 0,
    "camelcase": 0,
    "no-shadow": 0,
    "import/prefer-default-export": 0,
    "no-underscore-dangle": 0,
    "no-cond-assign": [2, "except-parens"],
    "no-param-reassign": ["error", { "props": false }],
    "no-unused-vars": ["error", { "args": "none" }]
  }
}
