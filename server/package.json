{
  "name": "SwipesAPI",
  "version": "0.30.1",
  "description": "Swipes REST API",
  "author": "Tihomir Valkanov <tihomir@swipesapp.com>",
  "dependencies": {
    "aws-sdk": "^2.162.0",
    "babel-runtime": "^6.26.0",
    "bluebird": "3.5.0",
    "bluebird-queue": "0.0.8",
    "body-parser": "1.x",
    "config": "^1.26.2",
    "content-disposition": "^0.5.2",
    "cookie-parser": "1.4.3",
    "cors": "^2.8.4",
    "express": "4.15.4",
    "express-session": "^1.15.5",
    "express-winston": "^2.4.0",
    "j2m": "^1.1.0",
    "jwt-simple": "^0.5.1",
    "mailchimp-api-v3": "^1.7.0",
    "mandrill-api": "^1.0.45",
    "mime-types": "^2.1.17",
    "moment": "^2.17.1",
    "multer": "^1.3.0",
    "object-hash": "^1.1.2",
    "randomstring": "^1.1.0",
    "request": "~2.81.0",
    "request-promise": "4.2.1",
    "rethinkdb": "^2.3.1",
    "rethinkdb-init": "0.2.2",
    "sha1": "^1.1.1",
    "sharp": "^0.18.2",
    "shortid": "^2.2.6",
    "slug": "^0.9.1",
    "stripe": "^4.25.0",
    "underscore": "1.8.x",
    "valjs": "0.0.28",
    "winston": "^2.3.1",
    "winston-aws-cloudwatch": "^2.0.0",
    "ws": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.16.0",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.7.0",
    "jest-cli": "^21.2.1",
    "nodemon": "^1.17.1",
    "npm-run-all": "^4.1.1",
    "pm2": "2.6.1",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.5.4"
  },
  "scripts": {
    "build": "babel src -d dist",
    "deploy:queue": "npm run build && eb deploy",
    "deploy-live:queue": "npm run build && eb deploy swipesqueue-live",
    "dev:api": "nodemon ./dist/index.api.js",
    "dev:queue": "nodemon ./dist/index.queue.js",
    "prestart": "rimraf ./dist",
    "production:api": "pm2 reload pm2_config.json --env production && pm2 save",
    "production:queue": "node ./dist/index.queue.js",
    "staging:api": "pm2 reload pm2_config.json --env staging && pm2 save",
    "staging:queue": "node ./dist/index.queue.js",
    "start": "npm run build && npm-run-all --parallel dev:api dev:queue watch",
    "test": "jest --env=node",
    "watch": "babel src -d dist --watch --skip-initial-build"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
