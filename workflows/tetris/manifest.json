{
	"manifest_version": 1,
	"name": "Tetris App",
	"identifier": "tetris",
	"version": "0.1",
	"description": "A Tetris game for Swipes",
	"index": "index.html",
	"icon": "images/logo-tetris.svg"
	"tables":[
		{
			"name": "highscore",
			"indexes": ["score"]
		}
	],
	"listenTo": [
		{
			"table": "highscore",
			"query": {
				"limit": 5,
				"order": "-score"
			}
		}
	]
}
